<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>User Directory</title>
    <link rel="stylesheet" href="styles.css">
  </head>
  <body>
    {{#users}}
    <div class="worker">
      <img src={{avatar}} alt="{{name}}'s image">
      <p class="worker-name">{{name}}</p>
      <p class="worker-job">{{job}}</p>
      <p class="worker-company">{{company}}</p>
    </div>
    {{/users}}


  </body>
</html>
